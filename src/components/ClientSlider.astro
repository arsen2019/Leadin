---
import ClientCarousel from './react_components/ClientCarousel.tsx';
import '../styles/global.css';
import React from "react";


const imageModules = import.meta.glob("/public/clients/*");
const imagePaths = Object.keys(imageModules).map((path) =>
    path.replace(/^\/public/, "")
);
console.log(imagePaths);

---

<div class="flex flex-col items-center">
    <ClientCarousel logos={imagePaths} client:only="react" />
</div>